  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "inspect": "electron-forge start --inspect-electron",
    "watch": "esbuild renderer.js --watch --platform=node --target=esnext --format=cjs --bundle --supported:dynamic-import=false --outfile=dist/renderer.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "start-watch": "concurrently --kill-others \"npm run start\" \"npm run watch\""
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "warodel": "^1.0.9"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@electron/remote": "^2.0.10",
    "electron": "^25.2.0",
    "electron-reloader": "^1.2.3",
    "esbuild": "0.18.11",
    "concurrently": "^8.2.0"
  }
}
